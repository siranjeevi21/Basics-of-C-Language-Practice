#include <stdio.h>

\* Grade System *\

int main() {
    char grade = 'A';
    switch(grade){
    case 'A':
        printf("You did great! ");
        break;
    case 'B':
        printf("You did Alright! ");
        break;
    case 'C':
        printf("You did poorly! ");
        break;
    case 'D':
        printf("you did bery bad");
        break;
    case 'F':
        printf("You Failed!");
        break;
    default:
        printf("Invalid Grade");
        
    }
